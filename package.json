{
  "name": "vessel-state",
  "version": "0.0.1-alpha",
  "description": "Takes a stream of data and decodes it into a reactive state for use in Vue components.",
  "repository": "https://github.com/ricardoboss/vessel-state",
  "authors": [
    "Ricardo Boss <contact@ricardoboss.de>"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "cross-env TS_NODE_TRANSPILE_ONLY=true NODE_ENV=test mocha --ui bdd --recursive --require ts-node/register test/**/*.test.ts",
    "test:watch": "npm run test -- --watch --watch-extensions ts --watch-files src,test --reporter min",
    "build": "vue-cli-service build --target lib src/main.ts && tsc --emitDeclarationOnly"
  },
  "devDependencies": {
    "@types/node": "^15.0.2",
    "@vue/cli-service": "^4.5.13",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "mocha": "^8.4.0",
    "ts-loader": "~8.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "vue-template-compiler": "^2.6.11"
  },
  "dependencies": {
    "extended-nmea": "2.0.0-beta11",
    "openbridge-css": "^0.2.1",
    "openbridge-web-components": "^0.2.1",
    "vue": "^2.6.12",
    "vuex": "^3.6.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
